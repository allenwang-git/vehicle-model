    <color rgba="0.65 0.65 0.65 1"/>
  </material>
  <material name="blue">
    <color rgba="0.1 0.1 0.5 1"/>
  </material> -->

  <!-- base relates to its projection on the ground by this offset:
    <origin xyz="0 0 0.31" rpy="0 0 0"/>
  -->


  <link name="base">
    <visual>
      <geometry>
        <mesh filename="https://github.com/allenwang-git/vehicle-model/blob/main/vehicle_model/meshes/lc.dae" scale="1.0 1.0 1.0"/>

      </geometry>
        <!-- <material name="aluminum"> 
          <color rgba="0.65 0.65 0.65 1"/>
        </material> -->
      <origin xyz="5 0 -0.5" rpy=" 0 0 3.1415926"/>
    </visual>
    <collision>
      <geometry>
        <box size="5.5 2.25 2"/>
      </geometry>
      <origin xyz="5 0 -0.5" rpy="0 0 3.1415926"/>
    </collision>
    <inertial>
      <!-- TODO: Get or calculate inertia values -1.570796-->
      <origin xyz="0 0 0.013" rpy="0 0 0"/>
      <mass value="0.25"/>
      <inertia ixx="0.0003350833" ixy="0" ixz="0"
        iyy="0.000560417" iyz="0"
        izz="0.000478833"/>
    </inertial>
  </link>

</robot>
